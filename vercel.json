{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "API_ID": "",
    "API_HASH": "",
    "BOT_TOKEN": "",
    "PYTHONUNBUFFERED": "1"
  },
  "functions": {
    "app.py": {
      "runtime": "python",
      "memory": 1024,
      "maxDuration": 30
    }
  }
}